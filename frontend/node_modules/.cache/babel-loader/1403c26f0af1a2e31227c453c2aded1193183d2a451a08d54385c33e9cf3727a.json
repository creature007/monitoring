{"ast":null,"code":"import moment from 'moment';\nexport default {\n  name: 'App',\n  data() {\n    return {\n      currentDateTime: ''\n    };\n  },\n  created() {\n    this.updateDateTime();\n    setInterval(this.updateDateTime, 1000);\n  },\n  methods: {\n    updateDateTime() {\n      this.currentDateTime = moment().format('DD.MM.YYYY HH:mm:ss');\n    }\n  }\n};","map":{"version":3,"names":["moment","name","data","currentDateTime","created","updateDateTime","setInterval","methods","format"],"sources":["C:\\Users\\user\\Desktop\\dastur\\hp\\monitoring\\frontend\\src\\App.vue"],"sourcesContent":["<template>\r\n  <div class=\"app-container\">\r\n    <nav class=\"sidebar\">\r\n      <div class=\"nav-brand\">\r\n        <i class=\"pi pi-server\"></i>\r\n        Monitoring\r\n      </div>\r\n      <div class=\"nav-links\">\r\n        <router-link to=\"/\" class=\"nav-link\">\r\n          <i class=\"pi pi-home\"></i>\r\n          <span>Dashboard</span>\r\n        </router-link>\r\n        <router-link to=\"/statistics\" class=\"nav-link\">\r\n          <i class=\"pi pi-chart-bar\"></i>\r\n          <span>Statistika</span>\r\n        </router-link>\r\n        <router-link to=\"/reports\" class=\"nav-link\">\r\n          <i class=\"pi pi-file\"></i>\r\n          <span>Hisobotlar</span>\r\n        </router-link>\r\n      </div>\r\n    </nav>\r\n\r\n    <main class=\"main-content\">\r\n      <header class=\"top-bar\">\r\n        <div class=\"search-box\">\r\n          <i class=\"pi pi-search\"></i>\r\n          <input type=\"text\" placeholder=\"Qidirish...\">\r\n        </div>\r\n        <div class=\"top-right\">\r\n          <span class=\"date-time\">{{ currentDateTime }}</span>\r\n          <i class=\"pi pi-bell\"></i>\r\n          <i class=\"pi pi-user\"></i>\r\n        </div>\r\n      </header>\r\n\r\n      <div class=\"content-area\">\r\n        <Toast />\r\n        <router-view v-slot=\"{ Component }\">\r\n          <transition name=\"fade\" mode=\"out-in\">\r\n            <component :is=\"Component\" />\r\n          </transition>\r\n        </router-view>\r\n      </div>\r\n    </main>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport moment from 'moment'\r\n\r\nexport default {\r\n  name: 'App',\r\n  data() {\r\n    return {\r\n      currentDateTime: ''\r\n    }\r\n  },\r\n  created() {\r\n    this.updateDateTime()\r\n    setInterval(this.updateDateTime, 1000)\r\n  },\r\n  methods: {\r\n    updateDateTime() {\r\n      this.currentDateTime = moment().format('DD.MM.YYYY HH:mm:ss')\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n:root {\r\n  --primary-bg: #1a237e;\r\n  --secondary-bg: #283593;\r\n  --card-bg: #1e1e2d;\r\n  --text-primary: #ffffff;\r\n  --text-secondary: #b3b3b3;\r\n  --accent-color: #3f51b5;\r\n  --success-color: #4caf50;\r\n  --warning-color: #ff9800;\r\n  --danger-color: #f44336;\r\n}\r\n\r\nbody {\r\n  margin: 0;\r\n  font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, 'Helvetica Neue', Arial, sans-serif;\r\n  background: var(--primary-bg);\r\n  color: var(--text-primary);\r\n}\r\n\r\n.app-container {\r\n  display: flex;\r\n  min-height: 100vh;\r\n}\r\n\r\n.sidebar {\r\n  width: 260px;\r\n  background: var(--card-bg);\r\n  padding: 1.5rem;\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 2rem;\r\n}\r\n\r\n.nav-brand {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 0.75rem;\r\n  font-size: 1.5rem;\r\n  font-weight: bold;\r\n  color: var(--text-primary);\r\n  padding-bottom: 1.5rem;\r\n  border-bottom: 1px solid rgba(255,255,255,0.1);\r\n}\r\n\r\n.nav-links {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 0.5rem;\r\n}\r\n\r\n.nav-link {\r\n  text-decoration: none;\r\n  color: var(--text-secondary);\r\n  padding: 0.75rem 1rem;\r\n  border-radius: 0.5rem;\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 0.75rem;\r\n  transition: all 0.3s ease;\r\n}\r\n\r\n.nav-link:hover,\r\n.nav-link.router-link-active {\r\n  background: var(--accent-color);\r\n  color: var(--text-primary);\r\n}\r\n\r\n.main-content {\r\n  flex: 1;\r\n  background: var(--primary-bg);\r\n  min-height: 100vh;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.top-bar {\r\n  background: var(--card-bg);\r\n  padding: 1rem 2rem;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.search-box {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 0.5rem;\r\n  background: rgba(255,255,255,0.1);\r\n  padding: 0.5rem 1rem;\r\n  border-radius: 0.5rem;\r\n}\r\n\r\n.search-box input {\r\n  background: none;\r\n  border: none;\r\n  color: var(--text-primary);\r\n  outline: none;\r\n  width: 300px;\r\n}\r\n\r\n.search-box input::placeholder {\r\n  color: var(--text-secondary);\r\n}\r\n\r\n.top-right {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 1.5rem;\r\n}\r\n\r\n.date-time {\r\n  color: var(--text-secondary);\r\n}\r\n\r\n.content-area {\r\n  padding: 2rem;\r\n  flex: 1;\r\n  overflow-y: auto;\r\n}\r\n\r\n.fade-enter-active,\r\n.fade-leave-active {\r\n  transition: opacity 0.3s ease;\r\n}\r\n\r\n.fade-enter-from,\r\n.fade-leave-to {\r\n  opacity: 0;\r\n}\r\n\r\n/* PrimeVue komponentlari uchun stillash */\r\n:deep(.p-card) {\r\n  background: var(--card-bg);\r\n  border-radius: 1rem;\r\n  box-shadow: 0 4px 6px rgba(0,0,0,0.1);\r\n}\r\n\r\n:deep(.p-card .p-card-title) {\r\n  color: var(--text-primary);\r\n}\r\n\r\n:deep(.p-card .p-card-content) {\r\n  color: var(--text-secondary);\r\n}\r\n\r\n:deep(.p-button) {\r\n  background: var(--accent-color);\r\n  border: none;\r\n  border-radius: 0.5rem;\r\n}\r\n\r\n:deep(.p-datatable) {\r\n  background: var(--card-bg);\r\n  border-radius: 1rem;\r\n  overflow: hidden;\r\n}\r\n\r\n:deep(.p-datatable .p-datatable-header) {\r\n  background: var(--card-bg);\r\n  border: none;\r\n}\r\n\r\n:deep(.p-datatable .p-datatable-thead > tr > th) {\r\n  background: var(--card-bg);\r\n  color: var(--text-primary);\r\n  border: none;\r\n  padding: 1rem;\r\n}\r\n\r\n:deep(.p-datatable .p-datatable-tbody > tr) {\r\n  background: var(--card-bg);\r\n  transition: background-color 0.3s;\r\n}\r\n\r\n:deep(.p-datatable .p-datatable-tbody > tr:hover) {\r\n  background: rgba(255,255,255,0.05);\r\n}\r\n\r\n:deep(.p-datatable .p-datatable-tbody > tr > td) {\r\n  border: none;\r\n  color: var(--text-secondary);\r\n}\r\n</style>\r\n"],"mappings":"AAiDA,OAAOA,MAAK,MAAO,QAAO;AAE1B,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,eAAe,EAAE;IACnB;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,cAAc,CAAC;IACpBC,WAAW,CAAC,IAAI,CAACD,cAAc,EAAE,IAAI;EACvC,CAAC;EACDE,OAAO,EAAE;IACPF,cAAcA,CAAA,EAAG;MACf,IAAI,CAACF,eAAc,GAAIH,MAAM,CAAC,CAAC,CAACQ,MAAM,CAAC,qBAAqB;IAC9D;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}